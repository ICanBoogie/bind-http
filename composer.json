{
	"name": "icanboogie/bind-http",
	"type": "library",
	"description": "Binds icanboogie/http to ICanBoogie",
	"homepage": "https://icanboogie.org/",
	"license": "BSD-3-Clause",
	"version": "5.0",
	"authors": [
		{
			"name": "Olivier Laviale",
			"email": "olivier.laviale@gmail.com",
			"homepage": "https://olvlvl.com/",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/ICanBoogie/bind-http/issues",
		"source": "https://github.com/ICanBoogie/bind-http"
	},
	"config": {
		"sort-packages": true
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"prefer-dist": true,
	"require": {
		"php": ">=7.2",
		"icanboogie/icanboogie": "^5.0",
		"icanboogie/http": "^4.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^8.5"
	},
	"autoload": {
		"psr-4": {
			"ICanBoogie\\Binding\\HTTP\\": "lib/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"ICanBoogie\\Binding\\HTTP\\": "tests/lib/"
		}
	},
	"extra": {
		"icanboogie": {
			"config-path": "config",
			"config-constructor": {
				"http_dispatchers": "ICanBoogie\\Binding\\HTTP\\Hooks::synthesize_dispatchers_config#http"
			}
		}
	},
	"scripts": {
		"post-autoload-dump": "ICanBoogie\\Autoconfig\\Hooks::on_autoload_dump"
	}
}
